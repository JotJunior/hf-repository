{
  "name": "jot/hf-repository",
  "description": "Projects related entities and repositories for elasticsearch mappings",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "elasticsearch",
    "entity",
    "repository",
    "etcd"
  ],
  "autoload": {
    "psr-4": {
      "Jot\\HfRepository\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Joao Zanon"
    }
  ],
  "extra": {
    "hyperf": {
      "config": "Jot\\HfRepository\\ConfigProvider",
      "annotations": {
        "scan": [
          "src"
        ]
      }
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/command": "~3.1.0",
    "hyperf/di": "~3.1.0",
    "hyperf/swagger": "^3.1",
    "jot/hf-elastic": "^0.16",
    "zircote/swagger-php": "^4.11",
    "hyperf/rate-limit": "^3.1",
    "hyperf/redis": "^3.1",
    "hyperf/http-server": "^3.1",
    "jot/hf-validator": "^0.1"
  }
}
